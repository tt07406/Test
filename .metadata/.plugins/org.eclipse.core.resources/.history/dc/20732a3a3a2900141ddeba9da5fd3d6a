	private static final long serialVersionUID = -7456160560536320060L;
	private UserService userService =new UserService();
	
	String kw;//查询关键字
	String pg;  //URL
	String cp; //为当前所在的页
	String ls; //每次显示的记录数
	long allRecorders; //查询到的全部记录数
	String url;
	int page;
	int size;
	int pid; //会员ID
	Users user; //会员信息
	List<Users> users; //会员列表
	
	String mid;//账号
	String password;//密码
	String info;//备注
	String realName;//真实姓名
	String sex;//性别
	String email;//电子邮件
	String phone;//电话号码
	String idNum;//身份证号

	/**
	 * @return
	 */
	public String execute() {
		// TODO Auto-generated method stub
		return SUCCESS;
	}
	
	public String list(){
		
		return ERROR;
	}
}